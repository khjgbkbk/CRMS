                        </TableRow>
                        <TableRow android:id="@+id/tableRow5" android:layout_width="wrap_content" android:layout_height="wrap_content">
                            <TextView android:id="@+id/textView3" android:layout_height="wrap_content" android:text="Name" android:layout_width="wrap_content"></TextView>
                            <EditText android:text="EditText" android:layout_height="wrap_content" android:id="@+id/editText2" android:layout_width="fill_parent"></EditText>
                        </TableRow>
                        <TableRow android:id="@+id/tableRow6" android:layout_width="wrap_content" android:layout_height="wrap_content">
