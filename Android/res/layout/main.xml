                            <EditText android:text="EditText" android:layout_height="wrap_content" android:id="@+id/EditText01" android:layout_gravity="center" android:layout_width="wrap_content"></EditText>
                        </TableRow>
                        <TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content">
